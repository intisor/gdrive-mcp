{
  "run": "npm start",
  "entrypoint": "server.js",
  "modules": ["nodejs-20"],
  "packager": {
	"language": "nodejs",
	"features": {
	  "packageSearch": true,
	  "guessImports": true,
	  "enabledForHosting": false
	}
  },
  "languages": {
	"javascript": {
	  "pattern": "**/{*.js,*.jsx,*.ts,*.tsx}",
	  "languageServer": {
		"start": "typescript-language-server --stdio"
	  }
	}
  },
  "nix": {
	"channel": "stable-23_11"
  },
  "env": {
	"NODE_ENV": "production"
  },
  "gitHubImport": {
	"requiredFiles": [".replit", "replit.nix"]
  },
  "deployment": {
	"run": ["npm", "start"]
  }
}
